				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"textTransform": "uppercase"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--blueprint-primary)",
					"fontFamily": "var(--wp--preset--font-family--open-sans)"
				}
			}
